{
  "hostname": "localhost",
  "port": 1337,
  "https": false,
  "log4js": {
    "appenders": {
      "console": { "type": "console" },
      "file": {
        "type": "dateFile",
        "filename": "logs/default",
        "pattern": "-yyyy-MM-dd.log",
        "alwaysIncludePattern": true,
        "maxLogSize": 1024,
        "backups": 3
      }
    },
    "categories": {
      "default": {
        "appenders": [ "console", "file" ],
        "level": "DEBUG"
      }
    }
  },
  "debug": {
    "nologin": true
  },
  "storage": {
    "root": "C:\\Users\\Lensual\\Source\\Repos\\"
  }
}